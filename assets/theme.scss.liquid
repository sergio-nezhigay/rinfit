//notification cookie
$notification-cookie-bg: color({{ settings.color_notification_cookie_bg | default: false }}, $theme);
$notification-cookie-button-info-c: color({{ settings.color_notification_cookie_button_info_c | default: false }}, $theme3);
$notification-cookie-button-info-h-c: color({{ settings.color_notification_cookie_button_info_h_c | default: false }}, $theme2);
$notification-cookie-button-close-c: color({{ settings.color_notification_cookie_button_close_c | default: false }}, $theme2);

//other
$slider-bullets-bg: color({{ settings.color_slider_bullets_bg | default: false }}, $theme);
$slider-bullets-h-bg: color({{ settings.color_slider_bullets_h_bg | default: false }}, $theme4);
$slider-bullets-h-opacity: {{ settings.slider_bullets_h_opacity }};
$slider-arrows-c: color({{ settings.color_slider_arrows_c | default: false }}, $slider-bullets-bg);
$slider-arrows-h-c: color({{ settings.color_slider_arrows_h_c | default: false }}, $slider-bullets-h-bg);
$slider-arrows-h-opacity: {{ settings.slider_arrows_h_opacity }};
$review-icons-c: color({{ settings.color_review_icons_c | default: false }}, $theme-primary);
$review-caption-c: color({{ settings.color_review_caption_c | default: false }}, $review-icons-c);
$price-c: color({{ settings.color_price_c | default: false }}, $theme);
$price-sale-1-c: color({{ settings.color_price_sale_1_c | default: false }}, $theme3);
$price-sale-2-c: color({{ settings.color_price_sale_2_c | default: false }}, $price-c);
$article-carousel-title-c: color({{ settings.color_article_carousel_title | default: false }}, $theme-link-c);
$article-subscription-section-bg: color({{ settings.color_subscription_section_bg | default: false }}, $theme-body);
$article-subscription-section-title-c: color({{ settings.color_subscription_section_title_c | default: false }}, $theme-head-c);
$article-subscription-section-paragraph-c: color({{ settings.color_subscription_section_paragraph_c | default: false }}, $theme-paragraph-c);
$home-sections-title-c: color({{ settings.color_home_sections_title_c | default: false }}, $theme-c);
$home-sections-title-h-c: color({{ settings.color_home_sections_title_h_c | default: false }}, $theme);
//global
//typography
body {
  background-color: $theme-body;
  color: $theme-c;
}
#MainContent {
  background-color: $theme-body;
}
h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {
  color: $theme-head-c;
}
a {
  color: $theme-link-c;
  &:visited,
  &:active,
  &:link {
    color: $theme-link-c;
  }
  @include desktop-hover-color(color, $theme-link-act-c);
}
a.link-unstyled {
  &:visited,
  &:active,
  &:link {
    color: initial;
  }
  @include desktop-hover-color(color, initial);
}
p, .paragraph {
  color: $theme-paragraph-c;
}
i {
  color: $theme;
}
.icon {
  fill: $theme;
}
blockquote {
  border-color: $theme;
  color: $theme;
  p {
    color: $theme;
  }
}
hr:not(.hr--clear) {
  border-top-color: $theme5;
}

.link-revert {
  @include link-color($theme-c);
  @include desktop-hover-color(color, $theme);
}

//bootstrap
.border {
  border-color: $theme5 !important;
}
.border-top {
  border-top-color: $theme5 !important;
}
.border-bottom {
  border-bottom-color: $theme5 !important;
}
.border-left {
  border-left-color: $theme5 !important;
}
.border-right {
  border-right-color: $theme5 !important;
}
.border-hover {
  @include desktop-hover-color(border-color, $theme !important);
}

//list
ul:not(.list-unstyled) li::before {
  background-color: $theme;
}
ol:not(.list-unstyled) li::before {
  color: $theme;
}

//table
.table, .responsive-table {
  th {
    color: $theme3;
  }
  td {
    color: $theme;
  }
  th, td, thead th, tbody + tbody {
    border-color: $theme5;
  }
}

//form
input[type="text"],
input[type="number"],
input[type="email"],
input[type="password"],
input[type="search"],
input[type="tel"],
textarea,
select {
  background-color: $input-bg;
  border-color: $input-bd;
  @include input-color($input-c);
  @include desktop-hover-color(border-color, $input-f-bd);
  &:focus {
    background-color: $input-f-bg;
    border-color: $input-f-bd;
  }
}
input {
  &.success {
    border-color: $theme-success;
  }
  &.error {
    border-color: $theme-error;
  }
}
label {
  color: $theme-head-c;
  &.success {
    color: $theme-success;
  }
  &.error {
    color: $theme-error;
  }
}
.note--success,
.note--error {
  &, * {
    color: $theme2;
  }
  ul li::before {
    background-color: $theme2;
  }
  ol li::before {
    color: $theme2;
  }
}
.note:not([class*='note--']) {
  background-color: $theme4;
  &::before {
    border-bottom-color: $theme4;
  }
}
.note--success {
  background-color: $theme-success;
  &::before {
    border-bottom-color: $theme-success;
  }
}
.note--error {
  background-color: $theme-error;
  &::before {
    border-bottom-color: $theme-error;
  }
}
.input-checkbox {
  color: $theme3;
  input:checked ~ span {
    color: $theme;
  }
  span {
    @include desktop-hover-color(color, $theme);
  }
  &:not(.input-checkbox--unbordered) input:checked + span {
    border-color: $theme !important;
  }
}
.input-boolean {
  color: $theme3;
  input + span {
    background-color: $theme4;
  }
  input + span span {
    background-color: $theme2;
    @include shadow(-1px 1px 0px 0px rgba(0, 0, 0, 0.05));
  }
  input:checked + span {
    background-color: $theme-primary;
  }
  input:checked ~ span {
    color: $theme;
  }
  &:hover input ~ span {
    @media (min-width: $screen-lg-up) {
      color: $theme;
    }
  }
}
.input-quantity [data-control] {
  @include desktop-hover-color(border-color, $theme !important);
}
.input-icon-button {
  .icon {
    fill: $theme6;
  }
  input {
    &:checked + .icon {
      fill: $theme;
    }
  }
}

//button
.btn-link {
  @include link-color($theme-c);
  @include desktop-hover-color(color, $theme-primary);
}
h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {
  &.btn-link {
    @include link-color($theme-head-c);
  }
}
.btn:not([class*="btn--invert"]):not([class*="btn--secondary"]):not([class*="btn--clean"]):not([class*="btn--dynamic-checkout"]):not([class*="btn--text"]),
.spr-summary-actions-newreview {
  @include colorize-btn($button-bg, $button-bd, $button-c, $button-h-bg, $button-h-bd, $button-h-c, $button-act-bg, $button-act-bd, $button-act-c);
}
.btn--invert {
  @include colorize-btn($button2-bg, $button2-bd, $button2-c, $button2-h-bg, $button2-h-bd, $button2-h-c, $button2-act-bg, $button2-act-bd, $button2-act-c);
}
.btn--secondary {
  @include colorize-btn($button3-bg, $button3-bd, $button3-c, $button3-h-bg, $button3-h-bd, $button3-h-c, $button3-act-bg, $button3-act-bd, $button3-act-c);
}
.btn--clean {
  @include colorize-btn($button5-bg, $button5-bd, $button5-c, $button5-h-bg, $button5-h-bd, $button5-h-c, $button5-act-bg, $button5-act-bd, $button5-act-c);
}
.dynamic-checkout__button--styled .shopify-payment-button__button {
  @include colorize-btn($button4-bg !important, $button4-bd !important, $button4-c !important, $button4-h-bg !important, $button4-h-bd !important, $button4-h-c !important, $button4-act-bg !important, $button4-act-bd !important, $button4-act-c !important);
  .shopify-cleanslate div[role="button"] {
    &,
    &:hover,
    &:focus {
      background-color: transparent !important;
    }
  }
}
.btn--transparent:not(:hover) {
  background-color: transparent !important;
}
.btn--transparent {
  @media (max-width: $screen-lg-down) {
    background-color: transparent !important;
  }
}
.btn--text {
  background-color: transparent;
  @include link-color($button-text-c);
  .icon {
    fill: $button-text-c;
  }
}

//label
.label {
  color: $label-c;
}
.label--sale {
  background-color: $label-sale-bg;
}
.label--new {
  background-color: $label-new-bg;
}
.label--hot {
  background-color: $label-hot-bg;
}
.label--out-stock {
  background-color: $label-out-stock-bg;
}
.label--in-stock {
  background-color: $label-in-stock-bg;
}
.in-stock {
  color: $theme7;
}
.out-stock {
  color: $theme8;
}

//spr
.spr-starrating,
.spr-icon {
  color: $review-icons-c;
}
.spr-badge-caption {
  color: $review-caption-c;
}
.spr-review-reportreview {
  @include link-color($theme3);
  @include desktop-hover-color(color, $theme-primary);
}

//range slider
.irs {
  .irs-line {
    background-color: $theme5;
  }
  .irs-bar {
    background-color: $theme;
  }
  .irs-min,
  .irs-max,
  .irs-from,
  .irs-to,
  .irs-single {
    background-color: transparent;
    color: $theme3;
  }
  .irs-slider {
    background-color: $theme;
    border-color: $theme;
    &.state_hover {
      background-color: $theme2;
    }
    @include desktop-hover-color(background-color, $theme2);
  }
}

//slider revolution
.slider-revolution {
  .tp-bullet {
    background-color: $slider-bullets-bg;
    &:hover {
      background-color: $slider-bullets-h-bg;
    }
  }
  .tp-loader.spinner2 {
    background-color: $slider-bullets-bg;
  }
  .tparrows.uranus::before {
    color: $slider-arrows-c;
  }
  .tparrows.uranus:hover::before {
    color: $slider-arrows-h-c;
  }
}

//tippy
.tippy-tooltip.shella-theme {
  background-color: $theme;
  color: $theme2;
}
.tippy-popper[x-placement^='top'] .tippy-tooltip.shella-theme .tippy-arrow {
  border-top-color: $theme;
}
.tippy-popper[x-placement^='bottom'] .tippy-tooltip.shella-theme .tippy-arrow {
  border-bottom-color: $theme;
}
.tippy-popper[x-placement^='left'] .tippy-tooltip.shella-theme .tippy-arrow {
  border-left-color: $theme;
}
.tippy-popper[x-placement^='right'] .tippy-tooltip.shella-theme .tippy-arrow {
  border-right-color: $theme;
}

//dropdown
.dropdown {
  @media (min-width: $screen-lg-up) {
    background-color: $theme2;
    @include shadow(0 4px 10px 0 rgba(0, 0, 0, 0.1));
  }
  span {
    @media (min-width: $screen-lg-up) {
      color: $theme3;
    }
  }
  a {
    @include link-color($theme3);
  }
  span, a {
    &:not([disabled]) {
      @include desktop-hover-color(color, $theme);
    }
    &[disabled] {
      opacity: 0.6;
    }
  }
  .selected,
  .active {
    color: $theme;
    @media (min-width: $screen-lg-up) {
      background-color: $theme4;
    }
    span {
      color: $theme;
    }
    a {
      @include link-color($theme);
    }
  }
}

//social media
.social-media a {
  @include link-color($theme3);
  &:hover {
    color: $theme;
  }
}

//breadcrumbs
.breadcrumbs {
  a {
    @include link-color($theme3);
    @include desktop-hover-color(color, $theme);
  }
}

//header
.header__tape {
  background-color: $header-tape-bg;
  p {
    color: $header-tape-c;
  }
}
.header__tape-close .icon {
  fill: $header-tape-btn-close-c;
}
.header__line-info {
  background-color: $header-info-line-bg;
  &, p {
    color: $header-info-line-c;
    a {
      @include link-color($header-info-line-c);
    }
  }
  .icon {
    fill: $header-info-line-c;
  }
}
.header__social-media {
  .icon {
    fill: $header-social-media-c;
  }
  a:hover .icon {
    fill: $header-social-media-h-c;
  }
}
.header__line-top {
  @media (min-width: $screen-lg-up) {
    background-color: $header-top-line-bg;
  }
  @media (max-width: $screen-lg-down) {
    background-color: $header-m-bg;
  }
}
.header__line-bottom {
  @media (min-width: $screen-lg-up) {
    background-color: $header-bottom-line-bg;
  }
}
.header__line-additional {
  background-color: $header-additional-line-bg;
  .info-line-text-list {
    color: $header-additional-line-c;
    span {
      color: $header-additional-line-highlighted-c;
    }
  }
}
.header__line-additional_button--register,
.header__line-additional_button--login {
  @include link-color($header-additional-line-account-c);
  @include desktop-hover-color(color, $header-additional-line-account-h-c);
}
.header__line-additional_button--services {
  @include link-color($header-additional-line-services-c);
  @include desktop-hover-color(color, $header-additional-line-services-h-c);
}
.header__search-form {
  input[type='search'] {
    background-color: $header-input-bg;
    border-color: $header-input-bd;
    @include input-color($header-input-c);
    &:focus {
      background-color: $header-input-f-bg;
      border-color: $header-input-f-bd;
    }
    @include desktop-hover-color(border-color, $header-input-f-bd);
  }
  label .icon {
    @media (min-width: $screen-lg-up) {
      fill: $theme-c;
    }
  }
}
.header__border {
  @media (min-width: $screen-lg-up) {
    border-bottom-color: $header-d-bd !important;
  }
  @media (max-width: $screen-lg-down) {
    border-bottom-color: $header-m-bd !important;
  }
}
.header__counter {
  @media (min-width: $screen-lg-up) {
    color: $header-counter-d-c;
  }
  @media (max-width: $screen-lg-down) {
    color: $header-counter-m-c;
  }
}
.header__btn-services,
.header__btn-account,
.header__btn-wishlist,
.header__btn-compare,
.header__btn-currency,
.header__select-currency select,
.header__btn-language {
  @media (min-width: $screen-lg-up) {
    @include link-color($header-btns-d-c);
  }
  @include desktop-hover-color(color, $header-btns-d-h-c);
  & > i .icon {
    @media (min-width: $screen-lg-up) {
      fill: $header-btns-d-c;
    }
  }
  &:hover > i .icon {
    @media (min-width: $screen-lg-up) {
      fill: $header-btns-d-h-c;
    }
  }
}
.header__btn-cart {
  @media (min-width: $screen-lg-up) {
    @include link-color($header-btn-cart-text-d-c);
  }
  @include desktop-hover-color(color, $header-btn-cart-d-h-c);
  & > i .icon {
    @media (min-width: $screen-lg-up) {
      fill: $header-btn-cart-d-c;
    }
  }
  &:hover > i .icon {
    @media (min-width: $screen-lg-up) {
      fill: $header-btns-d-h-c;
    }
  }
  &:hover .header__counter {
    @media (min-width: $screen-lg-up) {
      color: $header-btn-cart-text-d-c;
    }
  }
}
.header__btn-search {
  @media (min-width: $screen-lg-up) {
    @include link-color($header-btn-search-d-c);
  }
  @include desktop-hover-color(color, $header-btn-search-d-h-c);
  & > i .icon {
    @media (min-width: $screen-lg-up) {
      fill: $header-btn-search-d-c;
    }
  }
  &:hover > i .icon {
    @media (min-width: $screen-lg-up) {
      fill: $header-btn-search-d-h-c;
    }
  }
}
.header__btn-wishlist,
.header__btn-compare {
  &:hover .header__counter {
    @media (min-width: $screen-lg-up) {
      color: $header-counter-d-h-c;
    }
  }
}
.header__content--sticky {
  .header__btn-wishlist,
  .header__btn-compare,
  .header__btn-cart {
    @media (min-width: $screen-lg-up) {
      @include link-color($header-btns-sticky-c);
    }
    @include desktop-hover-color(color, $header-btns-sticky-h-c);
    & > i .icon {
      @media (min-width: $screen-lg-up) {
        fill: $header-btns-sticky-c;
      }
    }
    &:hover > i .icon {
      @media (min-width: $screen-lg-up) {
        fill: $header-btns-sticky-h-c;
      }
    }
    .header__counter {
      color: $header-btns-sticky-c;
    }
    &:hover .header__counter {
      @media (min-width: $screen-lg-up) {
        color: $header-btns-sticky-h-c;
      }
    }
  }
}
.header__btn-menu,
.header__btn-account,
.header__btn-wishlist,
.header__btn-compare,
.header__btn-cart,
.header__btn-currency,
.header__select-currency select,
.header__btn-language,
.header__btn-services,
.header__btn-search {
  @media (max-width: $screen-lg-down) {
    @include link-color($header-btns-m-c);
  }
  & > i .icon {
    @media (max-width: $screen-lg-down) {
      fill: $header-btns-m-c;
    }
  }
}
.header__nav {
  @media (max-width: $screen-lg-down) {
    background-color: $theme2;
  }
}
.header--transparent {
  .header__tape,
  .header__line-info,
  .header__line-top,
  .header__line-bottom,
  .header__nav {
    background-color: transparent !important;
  }
  .header__border {
    border: none !important;
  }
}

//menu
.menu::-webkit-scrollbar-track,
.menu--scroll-visible::after {
  @media (max-width: $screen-lg-down) {
    background-color: $theme4;
  }
}
.menu::-webkit-scrollbar-thumb {
  @media (max-width: $screen-lg-down) {
    background-color: $theme5;
  }
}
.menu-vertical-btn {
  @media (min-width: $screen-lg-up) {
    background-color: $header-menu-vertical-button-bg;
    color: $header-menu-vertical-button-c;
  }
  .icon {
    @media (min-width: $screen-lg-up) {
      fill: $header-menu-vertical-button-c;
    }
  }
}
.menu a {
  @media (max-width: $screen-lg-down) {
    @include link-color($theme3);
  }
}
.menu--vertical {
  @media (min-width: $screen-lg-up) {
    background-color: $header-menu-vertical-d-bg;
    @include shadow(0 4px 10px 0 rgba(0, 0, 0, 0.1));
  }
  @media (max-width: $screen-lg-down) {
    @include link-color($theme);
    border-bottom-color: $theme5;
  }
}
.menu--vertical.menu--fixed:not(.menu--items-visible) {
  @media (min-width: $screen-lg-up) {
    border-color: $theme5;
    @include shadow(none);
  }
}
.menu__back a {
  @media (max-width: $screen-lg-down) {
    @include link-color($theme);
  }
}
.menu__panel > .menu__item > a {
  @media (max-width: $screen-lg-down) {
    @include link-color($theme);
  }
}
.menu:not(.menu--vertical) .menu__panel > .menu__item > a {
  @media (min-width: $screen-lg-up) {
    @include link-color($header-menu-d-c);
  }
  .icon {
