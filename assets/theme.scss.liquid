$footer-button-back-to-top-m-bg: color({{ settings.color_footer_back_to_top_m_bg | default: false }}, $theme-primary);
$footer-button-back-to-top-m-c: color({{ settings.color_footer_back_to_top_m_c | default: false }}, $theme2);

