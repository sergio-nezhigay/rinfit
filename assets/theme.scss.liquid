$subscription-text-line-01-c: color({{ settings.color_subscription_line_01_c | default: false }}, $theme);
$subscription-text-line-02-c: color({{ settings.color_subscription_line_02_c | default: false }}, $theme3);
$subscription-text-line-03-c: color({{ settings.color_subscription_line_03_c | default: false }}, $theme3);
$subscription-text-line-04-c: color({{ settings.color_subscription_line_04_c | default: false }}, $theme3);
$subscription-dont-show-again-c: color({{ settings.color_subscription_dont_show_again_c | default: false }}, $theme3);
$subscription-close-text-c: color({{ settings.color_subscription_close_text_c | default: false }}, $theme);
