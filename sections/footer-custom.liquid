<footer id="shopify-section-{{ section.id }}" class="pp-footer {{ section.settings.custom_classes }}">
  <div class="pp-footer-main">
    <div class="pp-footer-container">
      <div class="pp-footer-left">
        {% if section.settings.logo_image %}
          <div class="pp-footer-logo">
            <img src="{{ section.settings.logo_image | image_url: width: 200 }}" alt="{{ section.settings.logo_alt | default: shop.name }}" loading="lazy" decoding="async" width="109" height="42">
          </div>
        {% endif %}
        {% if section.settings.description != blank %}
          <div class="pp-footer-description">
            {{ section.settings.description }}
          </div>
        {% endif %}
        {% if section.settings.country_selector_enabled %}
          <div class="pp-footer-localization">
            <div class="localization-selectors">
              <div class="relative">
                {% render 'localization-selector',
                  type: 'country',
                  class: 'pp-footer-country-selector',
                  button_class: 'localization-toggle heading text-xxs link-faded',
                  popover_class: 'popover popover--top-start color-scheme color-scheme--dialog'
                %}
              </div>
            </div>
          </div>
        {% endif %}
        {% assign social_blocks = section.blocks | where: 'type', 'social' %}
        {% if social_blocks.size > 0 %}
          <div class="pp-footer-socials">
            {% for block in social_blocks %}
              {% if block.settings.url != blank %}
                <a href="{{ block.settings.url }}" class="pp-footer-social-link" target="_blank" rel="noopener noreferrer" {{ block.shopify_attributes }}>
                  {% if block.settings.icon_svg != blank %}
                    {{ block.settings.icon_svg }}
                  {% elsif block.settings.icon_image %}
                    <img src="{{ block.settings.icon_image | image_url: width: 32 }}" alt="{{ block.settings.platform | default: 'Social' }}" loading="lazy" decoding="async" width="32" height="32">
                  {% endif %}
                </a>
              {% endif %}
            {% endfor %}
          </div>
        {% endif %}
      </div>
      <div class="pp-footer-right">
        {% assign nav_blocks = section.blocks | where: 'type', 'navigation' %}
        {% for block in nav_blocks %}
          {% if block.settings.title != blank %}
            <div class="pp-footer-nav-column" {{ block.shopify_attributes }}>
              <h3 class="pp-footer-nav-title">{{ block.settings.title }}</h3>
              {% if block.settings.menu != blank %}
                <nav class="pp-footer-nav-list">
                  {% for link in linklists[block.settings.menu].links %}
                    <a href="{{ link.url }}" class="pp-footer-nav-link">{{ link.title }}</a>
                  {% endfor %}
                </nav>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>

    <div class="pp-footer-mobile-stack --mobile-only">
      {% if section.settings.country_selector_enabled %}
        <div class="pp-footer-localization">
          <div class="localization-selectors">
            <div class="relative">
              {% render 'localization-selector',
                type: 'country',
                class: 'pp-footer-country-selector',
                button_class: 'localization-toggle heading text-xxs link-faded',
                popover_class: 'popover popover--top-start color-scheme color-scheme--dialog'
              %}
            </div>
          </div>
        </div>
      {% endif %}
      {% if social_blocks.size > 0 %}
        <div class="pp-footer-socials">
          {% for block in social_blocks %}
            {% if block.settings.url != blank %}
              <a href="{{ block.settings.url }}" class="pp-footer-social-link" target="_blank" rel="noopener noreferrer" {{ block.shopify_attributes }}>
                {% if block.settings.icon_svg != blank %}
                  {{ block.settings.icon_svg }}
                {% elsif block.settings.icon_image %}
                  <img src="{{ block.settings.icon_image | image_url: width: 32 }}" alt="{{ block.settings.platform | default: 'Social' }}" loading="lazy" decoding="async" width="32" height="32">
                {% endif %}
              </a>
            {% endif %}
          {% endfor %}
        </div>
      {% endif %}
      <div class="pp-footer-legal">
        {% assign legal_blocks = section.blocks | where: 'type', 'legal' %}
        {% for block in legal_blocks %}
          {% if block.settings.title != blank and block.settings.url != blank %}
            <a href="{{ block.settings.url }}" class="pp-footer-legal-link" {{ block.shopify_attributes }}>
              {{ block.settings.title }}
            </a>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>

  <div class="pp-footer-bottom">
    <div class="pp-footer-bottom-container">
      <div class="pp-footer-copyright">
        {% if section.settings.copyright_text != blank %}
          <span>{{ section.settings.copyright_text }}</span>
        {% endif %}
        {% if section.settings.rights_text != blank %}
          <span>{{ section.settings.rights_text }}</span>
        {% endif %}
      </div>
      <div class="pp-footer-legal">
        {% assign legal_blocks = section.blocks | where: 'type', 'legal' %}
        {% for block in legal_blocks %}
          {% if block.settings.title != blank and block.settings.url != blank %}
            <a href="{{ block.settings.url }}" class="pp-footer-legal-link" {{ block.shopify_attributes }}>
              {{ block.settings.title }}
            </a>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>
</footer>

<style>
  .pp-footer {
    width: 100%;
  }

  .pp-footer-main {
    background-color: #e9e4e1;
    color: #373a36;
    font-family: 'Kumbh Sans', sans-serif;
    padding: 40px 80px;
  }

  .pp-footer-container {
    display: flex;
    gap: 521px;
    align-items: flex-start;
    justify-content: flex-start;
    width: 100%;
  }

  .pp-footer-left {
    display: flex;
    flex-direction: column;
    gap: 20px;
    width: 197px;
    flex-shrink: 0;
  }

  .pp-footer-logo img {
    width: 109px;
    height: 42px;
    object-fit: contain;
  }

  .pp-footer-description {
    font-size: 12px;
    line-height: 16px;
    color: rgba(55, 57, 54, 0.6);
    font-weight: 400;
  }

  .pp-footer-localization {
    width: 100%;
  }

  .pp-footer-localization .localization-selectors {
    width: 100%;
  }

  .pp-footer-localization .localization-toggle {
    background: #ffffff;
    border: 1px solid rgba(55, 57, 54, 0.2);
    border-radius: 5px;
    padding: 10px;
    min-width: 193px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-family: 'Kumbh Sans', sans-serif;
    font-size: 14px;
    color: #373a36;
    text-decoration: none;
    cursor: pointer;
    text-transform: capitalize;
  }

  .pp-footer-socials {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
  }

  .pp-footer-social-link {
    width: 32px;
    height: 32px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #373a36;
    text-decoration: none;
    transition: opacity 0.2s ease;
  }

  .pp-footer-social-link:hover,
  .pp-footer-social-link:focus {
    opacity: 0.7;
  }

  .pp-footer-social-link svg {
    width: 100%;
    height: 100%;
  }

  .pp-footer-right {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    flex: 1;
    gap: 40px;
  }

  .pp-footer-nav-column {
    display: flex;
    flex-direction: column;
    gap: 20px;
    flex-shrink: 0;
  }

  .pp-footer-nav-title {
    font-size: 14px;
    font-weight: 600;
    color: #373a36;
    margin: 0;
    text-transform: capitalize;
  }

  .pp-footer-nav-list {
    display: flex;
    flex-direction: column;
    gap: 15px;
  }

  .pp-footer-nav-link {
    font-size: 14px;
    font-weight: 400;
    color: #373a36;
    text-decoration: none;
    text-transform: capitalize;
    transition: opacity 0.2s ease;
  }

  .pp-footer-nav-link:hover,
  .pp-footer-nav-link:focus {
    opacity: 0.7;
  }

  .pp-footer-bottom {
    background-color: #4f4e4e;
    padding: 10px 80px;
  }

  .pp-footer-bottom-container {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
  }

  .pp-footer-copyright {
    display: flex;
    align-items: center;
    gap: 30px;
    width: auto;
  }

  .pp-footer-copyright span {
    font-size: 12px;
    font-weight: 500;
    color: #ffffff;
    text-transform: capitalize;
    white-space: nowrap;
  }

  .pp-footer-legal {
    display: flex;
    align-items: center;
    gap: 50px;
    justify-content: space-between;
    flex-wrap: wrap;
  }

  .pp-footer-legal-link {
    font-size: 12px;
    font-weight: 500;
    color: #373a36;
    text-decoration: none;
    text-transform: capitalize;
    white-space: nowrap;
    transition: opacity 0.2s ease;
  }

  .pp-footer-bottom .pp-footer-legal .pp-footer-legal-link {
    color: #ffffff;
  }

  .pp-footer-mobile-stack.--mobile-only {
    display: none;
  }

  @media (max-width: 768px) {
    .pp-footer-main {
      padding: 20px 16px;
    }

    .pp-footer-container {
      flex-direction: column;
      gap: 24px;
      align-items: flex-start;
    }

    .pp-footer-left {
      width: 100%;
      gap: 16px;
    }

    .pp-footer-right {
      display: grid !important;
      grid-template-columns: repeat(2, minmax(0, 1fr));
      column-gap: 24px;
      row-gap: 20px;
      width: 100%;
    }

    .pp-footer-nav-column {
      min-width: 0;
    }

    .pp-footer-nav-title {
      margin: 0 0 8px 0;
      line-height: 20px;
      font-weight: 600;
    }

    .pp-footer-nav-list {
      display: grid;
      gap: 12px;
    }

    .pp-footer-nav-link {
      line-height: 20px;
      text-decoration: none;
    }

    .pp-footer-left .pp-footer-localization,
    .pp-footer-left .pp-footer-socials {
      display: none !important;
    }

    .pp-footer-mobile-stack.--mobile-only {
      display: grid;
      row-gap: 16px;
      margin-top: 8px;
    }

    .pp-footer-mobile-stack .pp-footer-socials {
      justify-content: space-between;
    }

    .pp-footer-mobile-stack .pp-footer-legal {
      gap: 16px;
      justify-content: flex-start;
    }

    .pp-footer-bottom {
      padding: 10px 16px;
    }

    .pp-footer-bottom-container {
      flex-direction: column;
      gap: 8px;
      align-items: center;
    }

    .pp-footer-bottom .pp-footer-legal {
      display: none !important;
    }
  }

  @media (min-width: 769px) {
    .pp-footer-mobile-stack.--mobile-only {
      display: none !important;
    }
  }
</style>

