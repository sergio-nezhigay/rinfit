<style>
    .header__tape {
        min-height: {{ section.settings.tape_min_height }}px;
    }
    @media (max-width: 540px) {
        .header__logo svg {
            max-width: {{ section.settings.logo_max_width_mobile }}px;
        }
        {%- if section.settings.menu_main_mobile_min_height > 0 -%}
        .menu--main .menu__panel {
            min-height: {{ section.settings.menu_main_mobile_min_height }}px;
        }
        {%- endif -%}
        {%- if section.settings.menu_secondary_mobile_min_height > 0 -%}
        .menu--secondary .menu__panel {
            min-height: {{ section.settings.menu_secondary_mobile_min_height }}px;
        }
        {%- endif -%}
    }
    @media (min-width: 1025px) {
        .header__line-info {
            min-height: {{ section.settings.info_line_min_height }}px;
        }
        .header__line-top {
            min-height: {{ section.settings.top_line_min_height }}px;
        }
        .header__line-bottom {
            min-height: {{ section.settings.bottom_line_min_height }}px;
        }
        .header__bottom-tape {
            min-height: {{ section.settings.bottom_tape_height }}px;
        }
        .menu:not(.menu--vertical) .menu__panel--on-a-par {
            margin-left: -{{ section.settings.menu_items_paddings_horizontal }}px;
            margin-right: -{{ section.settings.menu_items_paddings_horizontal }}px;
        }
        .menu:not(.menu--vertical) .menu__panel > .menu__item > a {
            padding-left: {{ section.settings.menu_items_paddings_horizontal }}px;
            padding-right: {{ section.settings.menu_items_paddings_horizontal }}px;
        }
    }
    {%- if section.settings.logo_adaptive -%}
        @media (min-width: 1025px) and (max-width: {{ section.settings.logo_adaptive_max_width }}px) {
            .header__logo--center {
                position: relative !important;
                width: 100% !important;
                margin-right: 25px;
                order: -1;
            }
            .header__logo--center svg {
                width: auto !important;
            }
        }
    {%- endif -%}
</style>

{%- if section.settings.sticky == 'desktop_and_mobile' or section.settings.sticky == 'desktop' -%}
    {%- if section.settings.type != '3' and section.settings.type != 'custom' and section.settings.type != '4' and section.settings.desktop_sticky_type == 'slim' -%}
        {%- assign need_sticky_sidebar = true -%}
        {%- if section.settings.show_sticky_logo -%}
            {%- assign need_sticky_logo = true -%}
        {%- endif -%}
    {%- elsif section.settings.type == '3' and section.settings.sticky_logo != blank -%}
        {%- if section.settings.show_sticky_logo -%}
            {%- assign need_sticky_logo = true -%}
        {%- endif -%}
    {%- elsif section.settings.type == 'custom' and section.settings.sticky_logo != blank -%}
        {%- if section.settings.show_sticky_logo -%}
            {%- assign need_sticky_logo = true -%}
        {%- endif -%}
    {%- endif -%}
{%- endif -%}
<div data-section-id="{{ section.id }}" data-section-type="header"{% include 'layout-get-container-class' %}>
    <header id="header" class="header header--type-{{ section.settings.type }}{% if section.settings.enable_transparent and template contains 'index' %} header--transparent position-absolute top-0 w-100{% else %} position-lg-relative{% endif %}{% if section.settings.sticky != 'disable' %} js-header-sticky{% endif %}" data-js-sticky="{{ section.settings.sticky }}" data-js-desktop-sticky-sidebar="{% if need_sticky_sidebar %}true{% else %}false{% endif %}">
        {%- for block in section.blocks -%}
            {%- if block.type == 'tape' -%}
                <div class="d-none js-header-tape" data-js-show-once="{{ block.settings.show_once }}" data-js-delay="{{ block.settings.delay }}" data-js-cookies-life="{{ block.settings.cookies_life }}">
                    <div class="header__tape d-flex py-5">
                        <div class="container d-flex flex-center">
                            <div class="position-relative d-flex align-items-center px-35">
                                <p class="m-0 px-30 px-lg-0 text-center">{{ block.settings.content }}</p>
                                <i class="header__tape-close position-absolute right-0 cursor-pointer" data-js-action="close">{% include 'icon-theme-190' %}</i>
                            </div>
                        </div>
                    </div>
                    {%- if section.settings.show_tape_border -%}
                        <div class="header__border border-bottom d-none d-lg-block"></div>
                    {%- endif -%}
                </div>
            {%- elsif block.type == 'info_line' -%}
                {%- capture info_line_html -%}
                    <div class="header__line-info d-none d-lg-flex align-items-lg-center px-10 px-lg-0 py-lg-2">
                        <div class="{% if section.settings.type == '4' %}w-100 px-10 px-lg-20{% else %}container{% endif %} d-flex flex-column flex-lg-row align-items-center">
                            <div>
                                {%- if block.settings.page_content != blank -%}
                                    {% include 'content-load-template' content: pages[block.settings.page_content].content %}
                                {%- else -%}
                                    {% include 'content-load-template' content: block.settings.content %}
                                {%- endif -%}
                            </div>
                            {%- if block.settings.show_social_media -%}
                                <div class="header__social-media d-flex align-items-center ml-auto">
                                    {%- assign show_facebook = block.settings.show_facebook -%}
                                    {%- assign show_twitter = block.settings.show_twitter -%}
                                    {%- assign show_google = block.settings.show_google -%}
                                    {%- assign show_instagram = block.settings.show_instagram -%}
                                    {%- assign show_pinterest = block.settings.show_pinterest -%}
                                    {%- assign show_youtube = block.settings.show_youtube -%}
                                    {%- assign show_behance = block.settings.show_behance -%}
                                    {%- assign show_skype = block.settings.show_skype -%}
                                    {% include 'social-media' with type: 'row', social_enable_tooltip: block.settings.enable_tooltip %}
                                </div>
                            {%- endif -%}
                        </div>
                    </div>
                    {%- if section.settings.show_info_line_border -%}
                        <div class="header__border border-bottom d-none d-lg-block"></div>
                    {%- endif -%}
                {%- endcapture -%}
            {%- endif -%}
        {%- endfor -%}
        {%- capture btn_menu_html -%}
            {%- if section.settings.show_button_menu -%}
                <span class="header__btn-menu d-flex align-items-center d-lg-none mr-20 cursor-pointer js-popup-button" data-js-popup-button="navigation">
                    <i>{% include 'icon-theme-191' %}</i>
                </span>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture logo_html -%}
            <div class="header__logo{% if need_sticky_logo %} header__logo--sticky-hidden{% endif %} d-flex align-items-center w-100{% if section.settings.type == 'custom' %} mt-lg-10{% endif %}{% if section.settings.type == '3' or section.settings.type == 'custom' %} mr-lg-25{% elsif section.settings.type == '2' %} header__logo--center mx-lg-auto{% elsif section.settings.type == '4' %} header__logo--center position-lg-absolute h-100 top-lg-0{% endif %}" style="max-width: {{ section.settings.logo_width }}px;">
                {%- if template.name == 'index' -%}
                    <h1 class="logo h6 w-100 m-0{% if section.settings.logo != blank or section.settings.logo_svg %} text-hide{% endif %}" itemscope itemtype="http://schema.org/Organization"{% if need_sticky_sidebar and section.settings.sticky_logo == blank %} data-js-sticky-replace-element="logo"{% endif %}>
                {%- else -%}
                    <div class="logo h6 w-100 m-0{% if section.settings.logo != blank or section.settings.logo_svg %} text-hide{% endif %}" itemscope itemtype="http://schema.org/Organization"{% if need_sticky_sidebar and section.settings.sticky_logo == blank %} data-js-sticky-replace-element="logo"{% endif %}>
                {%- endif -%}
                        <a href="/" itemprop="url"{% if section.settings.logo != blank or section.settings.logo_svg %} class="d-block text-hide"{% endif %}>
                            {%- capture image_attr -%}itemprop="logo"{%- endcapture -%}
                            {%- capture image_size -%}{{ section.settings.logo_width | times: 2 }}x{%- endcapture -%}
                            {%- if section.settings.logo_svg -%}
                                {%- if section.settings.logo_svg_code != blank -%}
                                    {{ section.settings.logo_svg_code }}
                                {%- else -%}
                                    <img src="{{ 'logo.svg' | asset_url }}" class="lazyload" alt="{{ shop.name }}" {{ image_attr }}>
                                {%- endif -%}
                            {%- elsif section.settings.logo != blank -%}
                                {% include 'rimage' with image: section.settings.logo size: image_size scale: 2 alt: shop.name attr: image_attr %}
                            {%- endif -%}
                            {{ shop.name }}
                        </a>
                {%- if template.name == 'index' -%}
                    </h1>
                {%- else -%}
                    </div>
                {%- endif -%}
            </div>
        {%- endcapture -%}
        {%- capture btn_account_html -%}
            {%- if section.settings.show_button_account and shop.customer_accounts_enabled -%}
                <a href="/account" class="header__btn-account d-flex{% if section.settings.type == '5' %} d-lg-none{% endif %} align-items-center position-relative ml-25 ml-lg-15 js-popup-button"
                    data-js-popup-button="account"
                    {%- if section.settings.enable_tooltip %}
                        data-js-tooltip
                        data-tippy-content="{{ 'layout.header.tooltip.account' | t }}"
                        data-tippy-placement="bottom"
                        data-tippy-distance="6"
                    {%- endif -%}
                >
                    <i>{% include 'icon-theme-201' %}</i>
                </a>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture btn_wishlist_html -%}
            {%- if section.settings.show_button_wishlist -%}
                <div class="ml-25 ml-lg-15">
                    <a href="#" class="header__btn-wishlist d-flex align-items-center position-relative text-nowrap js-popup-button" data-js-popup-button="wishlist"{% if need_sticky_sidebar %} data-js-sticky-replace-element="wishlist"{% endif %}
                        {%- if section.settings.enable_tooltip %}
                            data-js-tooltip
                            data-tippy-content="{{ 'layout.header.tooltip.wishlist' | t }}"
                            data-tippy-placement="bottom"
                            data-tippy-distance="6"
                        {%- endif -%}
                    >
                        <i>{% include 'icon-theme-180' %}</i>
                        {% capture wishlist_count %}{% if customer %}{{ customer.metafields.wishlist | size }}{% else %}0{% endif %}{% endcapture %}
                        <span class="header__counter" data-js-wishlist-count="{{ wishlist_count }}">{{ wishlist_count }}</span>
                    </a>
                </div>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture btn_compare_html -%}
            {%- if section.settings.show_button_compare -%}
                <div class="ml-25 ml-lg-15">
                    <a href="#" class="header__btn-compare d-flex align-items-center position-relative text-nowrap js-popup-button" data-js-popup-button="compare-full"{% if need_sticky_sidebar %} data-js-sticky-replace-element="compare"{% endif %}
                        {%- if section.settings.enable_tooltip %}
                            data-js-tooltip
                            data-tippy-content="{{ 'layout.header.tooltip.compare' | t }}"
                            data-tippy-placement="bottom"
                            data-tippy-distance="6"
                        {%- endif -%}
                    >
                        <i>{% include 'icon-theme-039' %}</i>
                        {% capture compare_count %}{% if customer %}{{ customer.metafields.compare | size }}{% else %}0{% endif %}{% endcapture %}
                        <span class="header__counter" data-js-compare-count="{{ compare_count }}">{{ compare_count }}</span>
                    </a>
                </div>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture btn_cart_html -%}
            {%- if section.settings.show_button_cart -%}
                <div class="ml-25 ml-lg-15">
                    <a href="/cart" class="header__btn-cart position-relative d-flex align-items-center text-nowrap js-popup-button" data-js-popup-button="cart"{% if need_sticky_sidebar %} data-js-sticky-replace-element="cart"{% endif %}>
                        {% capture cart_icon_name %}icon-{{ section.settings.icon_cart | default: 'theme-109' }}{% endcapture %}
                        <i class="mr-lg-7">{% include cart_icon_name %}</i>
                        <span class="d-none d-lg-inline mt-lg-3 cart-count" data-js-cart-count-desktop="{{ cart.item_count }}"> {{ cart.item_count }}</span>
                        <span class="header__counter d-lg-none cart-count" data-js-cart-count-mobile="{{ cart.item_count }}">{{ cart.item_count }}</span>
                    </a>
                </div>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture currencies_html -%}
            {%- if section.settings.show_button_currency and settings.show_multiple_currencies -%}
                {%- assign codes = 'USD,EUR,GBP,CAD,ARS,AUD,BBD,BDT,BSD,BHD,BRL,BOB,BND,BGN,ILS,MMK,KYD,CLP,CNY,COP,CRC,HRK,CZK,DKK,DOP,XCD,EGP,XPF,FJD,GHS,GTQ,GYD,GEL,HKD,HUF,ISK,INR,IDR,NIS,JMD,JPY,JOD,KZT,KES,KWD,LVL,LTL,MXN,MYR,MUR,MDL,MAD,MNT,MZN,ANG,NZD,NGN,NOK,OMR,PKR,PYG,PEN,PHP,PLN,QAR,RON,RUB,SAR,RSD,SCR,SGD,SYP,ZAR,KRW,LKR,SEK,CHF,TWD,THB,TZS,TTD,TRY,UAH,AED,UYU,VEB,VND,ZMK,IQD' | split: ',' -%}
                {%- assign supported_codes = settings.supported_currencies | split: ' ' -%}
                {%- assign supported_currencies_full_names = settings.supported_currencies_full_names | split: '|' -%}
                {%- assign supported_currencies_short_names = settings.supported_currencies_short_names | split: '|' -%}
                {%- capture list -%}
                    {%- if supported_codes.size > 0 -%}
                        <ul class="list-unstyled w-100 px-10 px-lg-15 py-30 py-lg-15">
                            {%- for code in supported_codes -%}
                                {%- if codes contains code -%}
                                    {%- assign index = forloop.index | minus: 1 -%}
                                    <li{% if code == shop.currency %} class="active"{% endif %}{% if supported_currencies_short_names[index] %} data-button-name="{{ supported_currencies_short_names[index] }}"{% endif %} data-currency-code="{{ code }}"><a href="#{{ code }}">{{ supported_currencies_full_names[index] | default: code }}</a></li>
                                    {%- if code == shop.currency -%}
                                        {%- assign shop_currency = supported_currencies_short_names[index] -%}
                                    {%- endif -%}
                                {%- endif -%}
                            {%- endfor -%}
                        </ul>
                    {%- endif -%}
                {%- endcapture -%}
                <div class="position-lg-relative d-none d-lg-block ml-lg-15 mt-lg-3" data-js-position-desktop="currencies">
                    <div class="js-position{% if supported_codes.size > 1 %} js-dropdown js-currencies-list{% endif %}" data-js-position-name="currencies">
                        <div class="header__btn-currency position-relative d-none d-lg-flex align-items-lg-center cursor-pointer" data-js-dropdown-button>
                            <span data-current-currency>{{ shop_currency | default: shop.currency }}</span>
                            {%- if supported_codes.size > 1 -%}
                                <i>{% include 'icon-theme-229' %}</i>
                            {%- endif -%}
                        </div>
                        {%- if supported_codes.size > 0 -%}
                            <div class="header__dropdown dropdown d-lg-none position-lg-absolute top-lg-100 right-lg-0" data-js-dropdown>
                                {{ list }}
                            </div>
                        {%- endif -%}
                    </div>
                </div>
            {%- elsif section.settings.show_button_currency and shop.enabled_currencies.size > 1 -%}
                <div class="position-lg-relative d-none d-lg-block ml-lg-15 mt-lg-3" data-js-position-desktop="currencies">
                    <div class="js-position select position-relative js-dropdown js-select js-currencies-form" data-js-position-name="currencies">
                        <div class="header__select-currency d-flex align-items-center" data-js-dropdown-button>
                            {% form 'currency', class: 'mb-0' %}
                                <select name="currency" class="d-none d-lg-block position-lg-relative h-auto p-0 pr-15 mb-0 border-0 cursor-pointer">
                                    {%- assign dropdown_options = null -%}
                                    {%- for currency in shop.enabled_currencies -%}
                                        {%- if currency == cart.currency -%}
                                            {%- assign is_selected = true -%}
                                        {%- else -%}
                                            {%- assign is_selected = false -%}
                                        {%- endif -%}
                                        <option value="{{currency.iso_code}}"{% if is_selected %} selected{% endif %}>{{currency.iso_code}}({{currency.symbol}})</option>
                                        {%- capture option -%}
                                        <span data-value="{{currency.iso_code}}"{% if is_selected %} class="selected"{% endif %}>{{currency.iso_code}} ({{currency.symbol}})</span>
                                        {%- endcapture -%}
                                        {%- assign dropdown_options = dropdown_options | append: option -%}
                                    {%- endfor -%}
                                </select>
                            {% endform %}
                            <i class="d-none d-lg-flex position-absolute right-0">{% include 'icon-theme-229' %}</i>
                        </div>
                        <div class="header__dropdown select__dropdown dropdown d-lg-none position-lg-absolute top-lg-100 right-lg-0" data-js-dropdown data-js-select-dropdown>
                            <div class="px-15 py-30 py-lg-15">
                                {{ dropdown_options }}
                            </div>
                        </div>
                    </div>
                </div>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture languages_html -%}
            {%- if section.settings.show_button_language -%}
                <div class="position-lg-relative d-none d-lg-block ml-lg-15 mt-lg-3" data-js-position-desktop="languages">
                    {%- case section.settings.language_app -%}
                        {% when 'weglot' %}
                            <div class="js-position js-dropdown js-languages-list d-none-important" data-js-position-name="languages">
                                <div class="header__btn-language position-relative d-none d-lg-flex align-items-lg-center cursor-pointer" data-js-dropdown-button>
                                    <span></span>
                                    <i>{% include 'icon-theme-229' %}</i>
                                </div>
                                <div class="header__dropdown dropdown d-lg-none position-lg-absolute top-lg-100 right-lg-0" data-js-dropdown>
                                    <ul class="list-unstyled w-100 px-10 px-lg-15 pb-30 py-lg-15"></ul>
                                </div>
                            </div>
                        {% when 'langify' %}
                            <div class="header__ly-languages-switcher px-15 px-lg-0 js-position" data-js-position-name="languages">
                                {% include 'ly-languages-switcher-theme-disign' %}
                            </div>
                        {% when 'none' %}
                            <div class="js-position js-dropdown js-languages-list" data-js-position-name="languages">
                                <div class="header__btn-language position-relative d-none d-lg-flex align-items-lg-center cursor-pointer" data-js-dropdown-button>
                                    <span>{{ section.settings.language_caption }}</span>
                                    <i>{% include 'icon-theme-229' %}</i>
                                </div>
                                <div class="header__dropdown dropdown d-lg-none position-lg-absolute top-lg-100 right-lg-0" data-js-dropdown>
                                    <ul class="list-unstyled w-100 px-15 pb-30 py-lg-15">
                                       {%- for language in linklists[section.settings.language_link_list].links -%}
                                            <li {% if language.active %}class="active"{% endif %}><a href="{{ language.url }}">{{ language.title }}</a></li>
                                       {%- endfor -%}
                                    </ul>
                                </div>
                            </div>
                    {%- endcase -%}
                </div>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture btn_search_html -%}
            {%- if section.settings.show_button_search and section.settings.type != '5' -%}
                <a href="/search" class="header__btn-search d-none d-lg-flex align-items-lg-center{% if section.settings.type == '1' %} ml-lg-auto{% else %} ml-25 ml-lg-15{% endif %} js-popup-button"
                   data-js-popup-button="navigation"
                    {%- if section.settings.enable_tooltip and section.settings.type != '1' %}
                       data-js-tooltip
                       data-tippy-content="{{ 'layout.header.tooltip.search' | t }}"
                       data-tippy-placement="bottom"
                       data-tippy-distance="6"
                    {%- endif -%}
                >
                {%- if section.settings.type == '1' -%}
                    <span class="mr-lg-7">{{ 'layout.header.search' | t }}</span>
                {%- endif -%}
                    <i>{% include 'icon-theme-168' %}</i>
                </a>
            {%- endif -%}
        {%- endcapture -%}
        {%- capture nav_html -%}
            <div class="header__nav d-none d-lg-flex{% if section.settings.type == '4' %} mr-lg-auto{% endif %}" data-js-position-desktop="menu">
                {%- if section.settings.type == '1' and section.settings.show_bottom_line_border -%}
                    {%- assign menu_offset = true -%}
                {%- elsif section.settings.type == '2' and section.settings.show_bottom_line_border -%}
                    {%- assign menu_offset = true -%}
                {%- elsif section.settings.type == '3' and section.settings.show_top_line_border -%}
                    {%- assign menu_offset = true -%}
                {%- elsif section.settings.type == 'custom' and section.settings.show_top_line_border -%}
                    {%- assign menu_offset = true -%}
                {%- elsif section.settings.type == '4' and section.settings.show_top_line_border -%}
                    {%- assign menu_offset = true -%}
                {%- elsif section.settings.type == '5' and section.settings.show_bottom_line_border -%}
                    {%- assign menu_offset = true -%}
                {%- endif -%}
                {%- if section.settings.type == '2' -%}
                    {%- assign menu_centered = true -%}
                {%- endif -%}
                {%- if section.settings.type != '5' -%}
                    {%- assign is_main_menu = true -%}
                {%- endif -%}
                {% include 'header-get-menu' with menu_offset: menu_offset is_main_menu: is_main_menu centered: menu_centered %}
            </div>
        {%- endcapture -%}
        {%- capture services_html -%}
            {%- if section.settings.show_button_services -%}
                <a href="#" class="header__btn-services position-relative d-flex{% if section.settings.type == '5' %} d-lg-none{% endif %} align-items-center ml-25 ml-lg-15 js-popup-button"
                   data-js-popup-button="services"
                   {%- if section.settings.enable_tooltip %}
                       data-js-tooltip
                       data-tippy-content="{{ 'layout.header.tooltip.services' | t }}"
                       data-tippy-placement="bottom"
                       data-tippy-distance="6"
                   {%- endif -%}
                >
                    <i>{% include 'icon-theme-172' %}</i>
                </a>
            {%- endif -%}
        {%- endcapture -%}
        {%- if need_sticky_sidebar -%}
            {%- capture sticky_sidebar_html -%}
                <div class="header__sticky-sidebar position-absolute d-none align-items-lg-center top-0 right-0 h-100 mr-15">
                    {%- if section.settings.show_button_wishlist -%}
                        <div class="ml-lg-15" data-js-sticky-replace-here="wishlist"></div>
                    {%- endif -%}
                    {%- if section.settings.show_button_compare -%}
                        <div class="ml-lg-15" data-js-sticky-replace-here="compare"></div>
                    {%- endif -%}
                    {%- if section.settings.show_button_cart -%}
                        <div class="ml-lg-15" data-js-sticky-replace-here="cart"></div>
                    {%- endif -%}
                </div>
            {%- endcapture -%}
        {%- endif -%}
        {%- if need_sticky_logo -%}
            {%- capture sticky_logo_html -%}
                <div class="header__sticky-logo d-none align-items-lg-center w-100 h-100{% if section.settings.type == '3' or section.settings.type == 'custom' %} header__sticky-logo--containerized mr-25{% else %} header__sticky-logo--displaced position-absolute top-0 left-0 py-6 ml-15{% endif %}"{% if section.settings.sticky_logo == blank %} data-js-sticky-replace-here="logo" style="max-width: {{ section.settings.logo_width }}px;"{% else %} style="max-width: {{ section.settings.sticky_logo_width }}px;"{% endif %}>
                    {%- if section.settings.sticky_logo != blank -%}
                        {%- if template.name == 'index' -%}
                            <h2 class="logo h6 w-100 m-0 text-hide" itemscope itemtype="http://schema.org/Organization">
                        {%- else -%}
                            <div class="logo h6 w-100 m-0 text-hide" itemscope itemtype="http://schema.org/Organization">
                        {%- endif -%}
                            <a href="/" itemprop="url" class="d-block text-hide">
