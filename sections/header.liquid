getComputedStyle(document.body).marginTop;
    {%- if section.settings.type == '5' and section.settings.search_ajax -%}
    Loader.requireh({type: "script", name: "search_ajax"});
    {%- endif -%}
    {%- if section.settings.sticky != 'disable' -%}
