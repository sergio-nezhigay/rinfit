{% capture style_bg %}{% if section.settings.color_bg != blank and section.settings.color_bg != 'rgba(0,0,0,0)' %} style="background-color: {{ section.settings.color_bg }};"{% endif %}{% endcapture %}
<div{% if section.settings.section_custom_class != blank %} class="{{ section.settings.section_custom_class }}"{% endif %}{{ style_bg }}>
    <div data-section-id="{{ section.id }}" data-section-type="home-builder"{% include 'layout-get-container-class' %}>
        {% include 'section-res-margins' %}
        {%- case section.settings.horizontal_align -%}
        {% when 'left' %}{% assign horizontal_align = 'justify-content-start' %}
        {% when 'center' %}{% assign horizontal_align = 'justify-content-center' %}
        {% when 'right' %}{% assign horizontal_align = 'justify-content-end' %}
        {%- endcase -%}
        {%- case section.settings.vertical_align -%}
        {% when 'top' %}{% assign vertical_align = 'align-items-start' %}
        {% when 'center' %}{% assign vertical_align = 'align-items-center' %}
        {% when 'bottom' %}{% assign vertical_align = 'align-items-end' %}
        {% when 'stretch' %}{% assign vertical_align = 'align-items-stretch' %}
        {%- endcase -%}
        <div class="{{ section_margins }}{% if section.settings.padding_top > 0 %} pt-{{ section.settings.padding_top }}{% endif %}{% if section.settings.padding_bottom > 0 %} pb-{{ section.settings.padding_bottom }}{% endif %}{% if section.settings.overflow_hidden %} overflow-hidden{% endif %}">
            {%- if section.settings.title != blank -%}
                <h4 class="{{ section.settings.title_style_tag }} mb-30 text-center">{{ section.settings.title | remove: '<p>' | remove: '</p>' | replace: '<a href', '<a class="text-underline" href' }}</h4>
            {%- endif -%}
            <div class="row {{ horizontal_align }} {{ vertical_align }}{% if section.settings.disable_paddings %} no-gutters{% endif %}">
                {%- assign block_types = 'slider,slider_02,instagram,promobox,textbox,products,custom_html,stack_builder,vertical_menu_spacer' | split: ',' -%}
                {%- for block in section.blocks -%}
                    {%- if block_types contains block.type -%}
                        {%- if block.settings.grid_responsive != blank -%}
                            {%- assign grid_responsive = block.settings.grid_responsive | split: ',' -%}
                            {% capture grid %}col col-{{ grid_responsive[4] }} col-sm-{{ grid_responsive[3] }} col-md-{{ grid_responsive[2] }} col-lg-{{ grid_responsive[1] }} col-xl-{{ grid_responsive[0] }}{% endcapture %}
                        {%- else -%}
                            {% capture grid %}col-12 col-sm-{{ block.settings.grid }} col-lg-{% if section.settings.enable_5_columns_grid %}2-5{% else %}{{ block.settings.grid }}{% endif %}{% endcapture %}
                        {%- endif -%}
                        {% include 'section-res-margins' with root: block %}
                        <div class="{% if block.type == 'vertical_menu_spacer' %}col-lg-3 d-none d-lg-block mt-lg-auto invisible pointer-events-none{% else %}{{ grid }}{{ section_margins }}{% if section.settings.vertical_align == 'stretch' %} d-flex flex-column align-items-stretch justify-content-center{% endif %}{% if block.settings.visible != blank %} {% if block.settings.visible == 'desktop' %}d-none d-md-{% if section.settings.vertical_align == 'stretch' %}flex{% else %}block{% endif %}{% elsif block.settings.visible == 'mobile' %}d-md-none{% endif %}{% endif %}{% endif %}">
                            {%- case block.type -%}
                            {%- when 'slider' -%}
                                {% include 'builder-slider' %}
                                <script>
                                    Loader.require({type: "style", name: "plugin_revolution"});
                                    Loader.require({type: "script", name: "plugin_revolution_slider"});
                                </script>
                            {%- when 'slider_02' -%}
                                {% include 'builder-slider-02' %}
                                <script>
                                    Loader.require({type: "style", name: "plugin_slick"});
                                </script>
                            {%- when 'instagram' -%}
                                {% include 'builder-instagram' %}
                            {%- when 'promobox' -%}
                                {% include 'promobox' %}
                            {%- when 'products' -%}
                                {%- if block.settings.title != blank -%}
                                    <h5 class="h4 mb-35">{{ block.settings.title }}</h5>
                                {%- endif -%}
                                {%- assign products = collections[block.settings.collection].products -%}
                                {%- for product in products limit: block.settings.max_products -%}
                                    {%- assign disable_lazyload = block.settings.disable_lazyload -%}
                                    {% include 'product-featured' %}
                                {%- endfor -%}
                            {%- when 'custom_html' -%}
                                <div class="rte">
                                    {%- if block.settings.page_content != blank -%}
                                        {% include 'content-load-template' content: pages[block.settings.page_content].content %}
                                    {%- else -%}
                                        {% include 'content-load-template' content: block.settings.content %}
                                    {%- endif -%}
                                </div>
                            {%- when 'stack_builder' -%}
                                <div class="rte">  
                                    {% assign sb_ring1_title = block.settings.sb_ring1_title %}
                                    {% assign sb_ring2_title = block.settings.sb_ring2_title %}
                                    {% assign sb_ring1_desc = block.settings.sb_ring1_desc %}                                    
                                    {% assign sb_ring2_desc = block.settings.sb_ring2_desc %}              
                                    {% assign sb_ring1_url = block.settings.sb_ring1_url %}                                    
                                    {% assign sb_ring2_url = block.settings.sb_ring2_url %}                                
                                    {% include 'stack_builder_hp' %}
                                </div>
                            {%- when 'vertical_menu_spacer' -%}
                                <div class="vertical-menu-spacer"></div>
                            {%- endcase -%}
                        </div>
                    {%- endif -%}
                {%- endfor -%}
            </div>
        </div>
    </div>
</div>

<script>
    Loader.require({type: "script", name: "home_builder"});
</script>
